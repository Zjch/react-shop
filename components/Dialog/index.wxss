@import '/assets/css/common.wxss';
.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  top: 0;
  left: 0;
  z-index: 9;
}
.dialog_wrapper {
  position: fixed;
  width: 80%;
  background: #fff;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 10;
  border-radius: 2px;
}
.dialog_wrapper .dialog_title {
  font-size: 16px;
  font-weight: bold;
  text-align: center;
  height: 50px;
  line-height: 50px;
  border-bottom: 0.5px solid #f9f9f9;
}
.dialog_wrapper .dialog_content {
  padding: 15px 0;
}
